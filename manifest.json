{
	"manifest_version": 2,
	"name": "Internshala Stipend Filter",
	"version": "0.1",
	"browser_action": {
			"default_title": "Set the minimum stipend",
			"default_popup": "popup.html"
	},
	"permissions": [
	 "activeTab",
	 "tabs",
	 "webNavigation"
	 ],
	"background": {
		"scripts": ["background.js"]
	},
	"content_scripts": [
	{
		"matches": [
			"https://internshala.com/*","http://internshala.com/*"
		],
		"js": ["script.js"],
		"run_at": "document_end"
	}
	]
}